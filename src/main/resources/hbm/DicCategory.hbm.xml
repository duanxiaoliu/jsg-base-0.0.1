<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="DIC_CATEGORY"  name="com.jsg.base.model.DicCategory">
    <id name="id" column="ID" type="java.lang.String">
      <generator class="uuid.hex"/>
    </id>
  	<property name="name" column="NAME" type="java.lang.String"/>
  	<property name="code" column="CODE" type="java.lang.String"/>
  	<property name="status" column="STATUS" type="java.lang.String"/>
  	<property name="comments" column="COMMENTS" type="java.lang.String"/>
  	<property name="cateGroryType" column="CATEGORY_TYPE" type="java.lang.String"/>
  	
  	
  	<property name="createTime" type="timestamp">
       <column name="CREATE_TIME" length="19" />
    </property>
    <property name="updateTime" type="timestamp">
       <column name="UPDATE_TIME" length="19" />
    </property>
    
  </class>
</hibernate-mapping>